#fn main_demo :i32
  v0 :any = call write_file "test_output.txt" "Hello from AILang!"
  v1 :text = call read_file "test_output.txt"
  log "info" "file contents: {0}" v1
  v2 :text = call env_get "PATH"
  v3 :bool = != v2 null
  log "info" "PATH exists: {0}" v3
  = 0

#test file_round_trip
  v0 :any = call write_file "test_tmp_ailang.txt" "round trip test"
  v1 :text = call read_file "test_tmp_ailang.txt"
  assert == v1 "round trip test"

#test env_get_path
  v0 :text = call env_get "PATH"
  assert != v0 null

#entry
  v0 :i32 = call main_demo
  = v0
